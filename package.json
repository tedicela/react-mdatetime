{
  "name": "react-mdatetime",
  "version": "1.3.0",
  "description": "React datetime picker powered by momentjs",
  "main": "./index.js",
  "scripts": {
    "start": "webpack-dev-server -c --port=8080",
    "build-js": "rm -rf lib && babel src --out-dir lib && cp -rf src/less/ lib/less && webpack -p",
    "build-css": "lessc --clean-css src/less/style.less dist/style.css",
    "build": "npm run build-js && npm run build-css",
    "deploy": "rm -rf .publish && npm run build && github-pages-deploy",
    "pretest": "npm run build",
    "clean": "rm dist/* && rm example/bundle*",
    "test": " echo 'test' "
  },
  "github-pages-deploy": {
    "src": "example"
  },
  "collaborators": [
    {
      "name": "Wang Zuo"
    },
    {
      "name": "Tedi Cela"
    }
  ],
  "license": "ISC",
  "peerDependencies": {
    "less": "^2.5.3",
    "less-loader": "^2.2.1",
    "less-plugin-clean-css": "^1.5.1",
    "style-loader": "^0.13.0",
    "css-loader": "^0.21.0",
    "moment": "^2.10.6",
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.1.2",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.0.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.1.2",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.21.0",
    "github-pages-deploy": "0.0.3",
    "json-loader": "^0.5.4",
    "less": "^2.5.3",
    "less-loader": "^2.2.1",
    "less-plugin-clean-css": "^1.5.1",
    "moment": "^2.17.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-test-renderer": "^16.0.0",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.3",
    "webpack-dev-server": "^1.14.0"
  },
  "dependencies": {
    "classnames": "^2.2.0",
    "lodash": "^4.17.4",
    "react-input-slider": "^4.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tedicela/react-mdatetime.git"
  },
  "keywords": [
    "react",
    "react-component",
    "input",
    "datetime",
    "picker",
    "moment"
  ],
  "bugs": {
    "url": "https://github.com/tedicela/react-mdatetime/issues"
  },
  "homepage": "https://github.com/tedicela/react-mdatetime#readme"
}
